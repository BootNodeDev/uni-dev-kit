{
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "tsconfig": "tsconfig.json",
  "name": "UniDevKit",
  "readme": "README.md",
  "theme": "default",
  "categorizeByGroup": true,
  "categoryOrder": ["Core", "Utils", "Helpers", "Types", "Constants", "Errors", "Hooks"],
  "searchInComments": true,
  "navigationLinks": {
    "GitHub": "https://github.com/BootNodeDev/uni-dev-kit"
  },
  "externalPattern": ["**/node_modules/**"],
  "excludeExternals": true,
  "exclude": ["**/node_modules/**", "**/dist/**"],
  "cleanOutputDir": true,
  "githubPages": true
}